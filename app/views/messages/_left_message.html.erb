<div class="message message__left">
  <div id="message-<%= message.id %>">
    <small>
      <strong><%= "#{message.user.username} " %></strong>
      <i class="datetime" data-original-date="<%= message.created_at %>"><%= message.created_at %></i>
    </small>
    <p><%= message.content %></p>
  </div>
</div>
